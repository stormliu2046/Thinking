<beans>
    <bean id="rateLimiter" class="com.example.demo.designpattern.factory.dependencyinjection.RateLimiter">
        <constructor-arg ref="redisCounter"/>
    </bean>

    <bean id="redisCounter" class="com.example.demo.designpattern.factory.dependencyinjection.RedisCounter">
        <constructor-arg type="String" value="127.0.0.1"/>
        <constructor-arg type="int" value="1234"/>
    </bean>
</beans>